Algoritmo sin_titulo
	//Contar del 1 al 10 con diferentes bucles
	
	Dimensionar numeros[10]
	Definir i Como Entero
	
	para i <- 1 hasta 10 Hacer
		numeros[i] <- i 
	FinPara
	
	i <- 1
	Mientras i <= 10
		escribir numeros[i]
		i <- i + 1
	FinMientras

	Para i <- 1 hasta 10 Hacer
		escribir numeros[i]
	
	FinPara
	
FinAlgoritmo
